    /*
    使用fastjson
    ValidateInterface#getParser()需返回 new Parser(JSON.class, Feature[].class)
    */
    name: {
        request: true,   //fastjson能写注释，key可以不加双引号
        "minLength": 1,
        "maxLength": 2000,
        "regex": "REGEX_CHAR",
        "message": "name:错误提示",  //fastjson支持在此加逗号
    },
    "birthday": {
        "request": true,
        "maxLength": 1,
//        "message": "birthday：必填"
    },
    "salaryNum": {
        "request": "false",
        "regex": "REGEX_FLOAT_10_2",
        "message": "薪水填写错误"
    },
    "girl": {
        "name": {
            "request": "false",
            "minLength": 2,
            "maxLength": 1,
//            "message": "2<=长度<=10"
        }
    },
    "hobbyList": {
        "request": "true",
        "maxLength": 20
    },
    "dreamList": {
        "txt": {
            "request": "true",
            "minLength": 1,
            "maxLength": 10
        },
        "exe": {
            "request": "true",
            "minLength": 2,
            "maxLength": 20
        },
        "money": {
            "request": "false",
            "minValue": 10,
            "maxValue": 1234567890123456,
            "regex": "REGEX_DECIMALS"
        }
    },

    "jsonKeyFormData": {
        "name": {
            "request": true,
            "minLength": 1,
            "maxLength": 20,
            "regex": "REGEX_CHAR",
            "message": "name错误"
        },
        "salaryNum": {
            "request": "false",
            "regex": "REGEX_FLOAT_10_2",
            "message": "薪水填写错误"
        }
    }
}














