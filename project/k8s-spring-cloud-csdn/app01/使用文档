chmod +x entrypoint.sh


docker build -t codingsoldier/eureka-server .

docker push codingsoldier/eureka-server




docker build -t codingsoldier/api-gateway .

docker push codingsoldier/api-gateway




docker build -t codingsoldier/app01 .

docker push codingsoldier/app01



docker build -t codingsoldier/app02 .

docker push codingsoldier/app02


























docker container rm -f eureka-server

docker run --name=eureka-server -e PORT=10001 codingsoldier/eureka-server




docker build -t codingsoldier/api-gateway .

docker container rm -f api-gateway

docker run --name=api-gateway codingsoldier/api-gateway





docker build -t codingsoldier/app01 .

docker container rm -f app01

docker run --name=app01 codingsoldier/app01




docker build -t codingsoldier/app02 .

docker container rm -f app02

docker run --name=app02 codingsoldier/app02























jstat -gc 1 5000 100
